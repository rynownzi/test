    if (targetingData && targetingData.ad_unit) {
      if (targetingData.ppid) {
        window.googletag.cmd.push(function() {
          window.googletag.pubads().setPublisherProvidedId(targetingData.ppid);
          window.googletag.enableServices();
          // If the server returns a personalized_ads flag, and that flag is false, then
          // we tell GAM to not request personalized ads.
          if ((typeof targetingData.personalized_ads !== 'undefined') &&
              targetingData.personalized_ads === false) {
                window.googletag.pubads().setRequestNonPersonalizedAds(1);
          }
        });
      }
      // request feed_position=1 from GAM
      function guid() {
        var s4 = function () {
          return Math.floor((1 + Math.random()) * 0x10000)
            .toString(16)
            .substring(1);
        };

        var guidStr = s4() + s4() + '-' + s4() + '-' + s4() + '-' + s4() + '-' + s4() + s4() + s4();
        return guidStr.toUpperCase();
      }

      var firstGamSlot = null;
      window.googletag.cmd.push(function() {
        firstGamSlot = window.googletag
            .defineSlot(targetingData.ad_unit, ['fluid', [1, 1]], guid());
        var kvData = targetingData.kv;
        for (var key in kvData) {
          if (kvData.hasOwnProperty(key) && kvData[key]) {
            firstGamSlot.setTargeting(key, kvData[key]);
          }
        }
        firstGamSlot.setTargeting('feed_position', '1');
        firstGamSlot.setTargeting('intended_slot', '1');
        firstGamSlot.addService(window.googletag.pubads());
        window.googletag.pubads().enableLazyLoad({
          fetchMarginPercent: 500,  // Fetch slots within 5 viewports
          renderMarginPercent: 0,  // Render slots when in current viewport
        });
        window.googletag.enableServices();
      });
    }

    var pbjs = pbjs || {};
    window.pbjs = pbjs;
    window.pbjs.que = pbjs.que || [];

    /**
     * CMP Stub
     *
     * @returns {*|Array} If called with no arguments, will return queue of commands
     */
    function stubCMP(command, parameter, callback) {
      __cmp.queue = __cmp.queue || [];

      if (command === 'ping' && callback){
        callback({'gdprAppliesGlobally': __cmp.false,
          'cmpLoaded': false}, true);
      } else {
        __cmp.queue.push({command, parameter, callback});
      }
    }

    if (typeof (__cmp) !== 'function') {
      window.__cmp = stubCMP;
    }
  </script>


    
